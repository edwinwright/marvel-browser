{
  "name": "marvel-browser",
  "version": "0.0.1",
  "description": "A React app for browsing the Marvel API.",
  "scripts": {
    "start": "node server.js",
    "build": "rimraf ./dist && webpack --config webpack.config.prod.js",
    "build:dev": "rimraf ./dist && webpack",
    "test": "NODE_PATH=./src mocha './src/**/*.spec.jsx' --compilers js:babel-register --require './test/unit/setup.js'",
    "test:watch": "npm test -- --watch",
    "test2": "karma start --single-run --no-auto-watch",
    "test2:watch": "karma start --auto-watch --no-single-run",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:watch": "esw src --watch",
    "lint:styles": "stylelint './src/**/*.scss' --syntax scss",
    "lint:scripts": "eslint './src/**/*.js'"
  },
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "foundation-sites": "^6.2.4-rc2",
    "humps": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.8.0",
    "lodash": "^4.16.4",
    "normalize.css": "^5.0.0",
    "normalizr": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.1.2",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.7.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "eslint-watch": "^2.1.13",
    "expect": "^1.8.0",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.10.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "url-loader": "^0.5.7"
  }
}
