{
  "name": "marvel-browser",
  "version": "0.0.1",
  "description": "A React app for browsing the Marvel API.",
  "scripts": {
    "start": "node server.js",
    "build": "rimraf ./dist && webpack --config webpack.config.prod.js",
    "build:dev": "rimraf ./dist && webpack",
    "test": "mocha src/**/*.spec.js --compilers js:babel-register --require test/unit/setup.js",
    "test:watch": "npm test -- --watch",
    "test2": "karma start --single-run --no-auto-watch",
    "test2:watch": "karma start --auto-watch --no-single-run",
    "lint": "eslint src",
    "lint:watch": "esw src --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "classnames": "^2.2.5",
    "foundation-sites": "^6.2.3",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.1.2",
    "react-router": "^2.7.0",
    "redux": "^3.1.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.3.13",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.0",
    "eslint-plugin-react": "^6.1.2",
    "eslint-watch": "^2.1.13",
    "expect": "^1.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.8.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.10.1",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.12.1"
  }
}
